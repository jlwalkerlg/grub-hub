{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.AspNetCore": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Elasticsearch",
        "Args": {
          "nodeUris": "http://localhost:9200",
          "indexFormat": "grubhub-logs-{0:yyyy.MM.dd}",
          "restrictedToMinimumLevel": "Warning"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341",
          "restrictedToMinimumLevel": "Warning"
        }
      }
    ]
  },
  "App": {
    "ServerUrl": "http://localhost:5000",
    "ClientUrl": "http://localhost:3000",
    "CorsOrigins": ["http://localhost:3000"]
  },
  "Database": {
    "ConnectionString": "User ID=*********; Password=*********; Host=127.0.0.1; Port=5432; Database=grubhub;"
  },
  "Geocoding": {
    "Driver": "Doogal",
    "GoogleApiKey": "*********"
  },
  "Stripe": {
    "SecretKey": "*********",
    "WebhookSigningSecret": "*********",
    "ConnectWebhookSigningSecret": "*********"
  },
  "Mail": {
    "FromAddress": "grubhub@grubhub.com",
    "FromName": "Grub Hub",
    "Username": "*********",
    "Password": "*********",
    "Host": "smtp.mailtrap.io",
    "Port": 587
  },
  "Aws": {
    "AccessKeyId": "*********",
    "SecretAccessKey": "*********",
    "Region": "eu-west-1",
    "Bucket": "grub-hub"
  },
  "Cache": {
    "Driver": "InMemory",
    "Redis": {
      "ConnectionString": "redis:6379",
      "InstanceName": "grubhub_"
    }
  },
  "Cap": {
    "Transport": {
      "Driver": "InMemory",
      "RabbitMq": {
        "HostName": "rabbitmq",
        "UserName": "*********",
        "Password": "*********",
        "Port": 5672
      }
    },
    "Storage": {
      "Driver": "InMemory"
    }
  }
}
